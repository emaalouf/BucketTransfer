{
  "name": "bucket-transfer-tool",
  "version": "1.0.0",
  "description": "Node.js tool to transfer files from DigitalOcean Spaces to AWS S3",
  "main": "transfer.js",
  "scripts": {
    "start": "node transfer.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop bucket-transfer",
